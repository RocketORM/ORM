    /**
     * @param array $values
     *
     * @return void
     */
    public function hydrate(array $values)
    { {%- for column in table.columns %}

        {% if not column.isRequired -%}
            if (isset($values['{{ column.name }}'])) {
            {{ "" }}
        {%- endif %}
        {%- if constant('\\Rocket\\ORM\\Model\\Map\\TableMap::COLUMN_TYPE_DATETIME') == column.type or
              constant('\\Rocket\\ORM\\Model\\Map\\TableMap::COLUMN_TYPE_DATE') == column.type
        -%}
            $this->{{ column.phpName }} = new \DateTime($values['{{ column.name }}']);
        {%- else -%}
            $this->{{ column.phpName }} = ({{ column.typeAsPhpDoc }}) $values['{{ column.name }}'];
        {%- endif -%}
        {% if not column.isRequired -%}
            {{ "" }}
        }
        {%- endif -%}
        {{ "\n" }}
    {%- endfor %}
    }
