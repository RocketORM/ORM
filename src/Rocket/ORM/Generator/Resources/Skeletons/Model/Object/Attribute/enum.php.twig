
    /**
     * The enum key value for the "{{ column.name }}" column
     *
     * @var array
     */
    protected static $enum{{ column.phpName(true) }} = [
        {%- for i,value in column.values -%}
            {{ "\n        " -}}
            {{ i }} => {{ value|type('string') ? "'" ~ value ~ "'" : value }}{{ not loop.last ? ',' : '' }}
        {%- endfor -%}
        {{ "\n    " -}}
    ];
